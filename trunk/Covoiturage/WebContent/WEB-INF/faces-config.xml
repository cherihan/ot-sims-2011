<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>annonce</managed-bean-name>
  <managed-bean-class>beans.Annonce</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>listofAnnonces</managed-bean-name>
  <managed-bean-class>beans.ListofAnnonces</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>user</managed-bean-name>
  <managed-bean-class>beans.BeansUser</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>ok</from-outcome>
   <to-view-id>/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/home_public.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>home_public.xhtml</display-name>
 	<from-view-id>/home_public.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/list_route.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>edit_profile.xhtml</display-name>
 	<from-view-id>/edit_profile.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/profile.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>profile.xhtml</display-name>
 	<from-view-id>/profile.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/edit_profile.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>home_public.xhtml</display-name>
 	<from-view-id>/home_public.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/create_account.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>login.xhtml</display-name>
 	<from-view-id>/login.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/home_public.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>create_account.xhtml</display-name>
 	<from-view-id>/create_account.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/home_public.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>create_account.xhtml</display-name>
 	<from-view-id>/create_account.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/home.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>home.xhtml</display-name>
 	<from-view-id>/home.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/list_route.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>home.xhtml</display-name>
 	<from-view-id>/home.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/home_public.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>profile.xhtml</display-name>
 	<from-view-id>/profile.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/home_public.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>list_route.xhtml</display-name>
 	<from-view-id>/list_route.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/info_route.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>info_route.xhtml</display-name>
 	<from-view-id>/info_route.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/list_route.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>home.xhtml</display-name>
 	<from-view-id>/home.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/add_route.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>add_route.xhtml</display-name>
 	<from-view-id>/add_route.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/list_route.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>list_route.xhtml</display-name>
 	<from-view-id>/list_route.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/add_route.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>profile.xhtml</display-name>
 	<from-view-id>/profile.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/list_route.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>info_route.xhtml</display-name>
 	<from-view-id>/info_route.xhtml</from-view-id>
 	<navigation-case>
 		<to-view-id>/profile.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
 </application>
</faces-config>
